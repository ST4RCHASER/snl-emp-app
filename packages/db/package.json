{
  "name": "@snl-emp/db",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env -- prisma generate --schema=../../prisma/schema.prisma",
    "db:push": "dotenv -e ../../.env -- prisma db push --schema=../../prisma/schema.prisma",
    "migrate": "dotenv -e ../../.env -- prisma migrate dev --schema=../../prisma/schema.prisma",
    "studio": "dotenv -e ../../.env -- prisma studio --schema=../../prisma/schema.prisma",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.2.1",
    "@prisma/client": "^6.2.1",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.2.1",
    "typescript": "^5.7.3"
  }
}
